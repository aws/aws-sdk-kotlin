{
  "version": "1.0",
  "projections": {
    "aws-restjson": {
      "transforms": [
        {
          "name": "includeServices",
          "args": {
            "services": ["aws.protocoltests.restjson#RestJson"]
          }
        }
      ],
      "plugins": {
        "kotlin-codegen": {
          "service": "aws.protocoltests.restjson#RestJson",
          "module": "aws.sdk.kotlin.runtime.protocoltest.awsrestjson",
          "moduleVersion": "1.0",
          "build":  {
              "rootProject": true,
              "optInAnnotations": ["software.aws.clientrt.util.InternalAPI", "aws.sdk.kotlin.runtime.InternalSdkApi"]
          }
        }
      }
    },
    "aws-json-10": {
      "transforms": [
        {
          "name": "includeServices",
          "args": {
            "services": ["aws.protocoltests.json10#JsonRpc10"]
          }
        }
      ],
      "plugins": {
        "kotlin-codegen": {
          "service": "aws.protocoltests.json10#JsonRpc10",
          "module": "aws.sdk.kotlin.runtime.protocoltest.awsjson10",
          "moduleVersion": "1.0",
          "build":  {
            "rootProject": true,
            "optInAnnotations": ["software.aws.clientrt.util.InternalAPI", "aws.sdk.kotlin.runtime.InternalSdkApi"]
          }
        }
      }
    },
    "aws-json-11": {
      "transforms": [
        {
          "name": "includeServices",
          "args": {
            "services": ["aws.protocoltests.json#JsonProtocol"]
          }
        }
      ],
      "plugins": {
        "kotlin-codegen": {
          "service": "aws.protocoltests.json#JsonProtocol",
          "module": "aws.sdk.kotlin.runtime.protocoltest.awsjson11",
          "moduleVersion": "1.0",
          "build":  {
            "rootProject": true,
            "optInAnnotations": ["software.aws.clientrt.util.InternalAPI", "aws.sdk.kotlin.runtime.InternalSdkApi"]
          }
        }
      }
    }
  }
}
